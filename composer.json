{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^8.3",
        "symfony/skeleton": "*",
        "symfony/orm-pack": "*",
        "annotations": "*",
        "symfony/validator": "*",
        "nelmio/cors-bundle": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require-dev": {
        "symfony/maker-bundle": "^1.0"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    }
}